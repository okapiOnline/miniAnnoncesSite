<div class="well">
    {{ form_errors(form) }}
    {{ form_start(form) }}
    <div class="form-group">
        {{ form_label(form.title, null, {'label': 'Titre','label_attr': {'class': 'foo'}} )}}
        {{ form_widget(form.title, { 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="form-group">
        {{ form_label(form.categoryId, null, {'label': 'Categorie','label_attr': {'class': 'foo'}} )}}
        {{ form_widget(form.categoryId, { 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="form-group">
        {{ form_label(form.online, null, {'label': 'En ligne','label_attr': {'class': 'foo'}} )}}
        {{ form_widget(form.online, { 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="form-group">
        {{ form_label(form.content, null, {'label': 'Contenu de l\'article','label_attr': {'class': 'foo'}} )}}
        {{ form_widget(form.content, { 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="btn btn-success">
        <input type="submit" class="btn btn-success" >
    </div>
    {{ form_end(form) }}
</div>